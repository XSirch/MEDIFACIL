{
    "version": 2,
    "builds": [{
        "src": "healing/wsgi.py",
        "use": "@vercel/python"

    },
    {
        "src": "build_files.sh",
        "use": "@vercel/static-build",
        "config": {
          "distDir": "staticfiles_build"
        }
    }
],
    "routes": [
        {
            "src": "template/static/(.*)",
            "dest": "/static/$1"
          },
        {
            "handle": "filesystem" 
        },
        { "src": "/(.*)",
        "dest": "healing/wsgi.py"   
         }
    ]
}